{
    "compilerOptions": {
        "target": "esnext",
        "useDefineForClassFields": true,
        "module": "commonjs",
        "skipLibCheck": true,
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "removeComments": true,
        "noFallthroughCasesInSwitch": true
    },
    "exclude": ["./frontend", "./type-explorer"]
}
